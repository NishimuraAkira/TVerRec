{
	"manifest_version": 2,
	"name": "TVer/GYAO動画URLコピー",
	"author": "dongaba",
	"description": "TverとGYAOの動画を落とすURLをコピー",
	"version": "1.3.2",
	"icons": {
		"18": "icon.png"
	},
	"permissions": [
		"<all_urls>",
		"webRequest",
		"clipboardWrite"
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"content_scripts": [
			{
				"matches": [
					"http://tver.jp/*"
				],
				"js": [
					"adKiller-main.js"
				]
			},
			{
				"matches": [
					"https://tver.jp/*"
				],
				"js": [
					"adKiller-main.js"
				]
			}
		]
	}
}